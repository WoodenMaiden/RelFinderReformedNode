{
  "name": "relfinderreformed",
  "version": "1.0.0beta",
  "description": "",
  "main": "./dist/app.js",
  "scripts": {
    "prebuild": "npx tslint -c tslint.json -p tsconfig.json --fix",
    "build": "rm -rf dist/* ; npm run prebuild && tsc",
    "dev:start": "kill $(ss -lptn 'sport = :8080' | grep LISTEN | cut -d= -f2 | cut -d, -f1); npm-run-all build -s start",
    "dev": "npx nodemon --watch src -e ts,js,ejs --exec npm run dev:start",
    "start": "node . http://localhost:8888/sparql -c strict -p 8080 -l /dev/stdout /tmp/rfr_$(date --rfc-3339=seconds | sed 's/ /_/').log --loglevel TRACE",
    "test": "npx jest"
  },
  "keywords": [
    "API",
    "RDF",
    "Relfinder",
    "Graphs"
  ],
  "author": "Yann POMIE",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@types/express": "^4.17.13",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "graphology": "^0.23.2",
    "graphology-shortest-path": "^2.0.1",
    "install": "^0.13.0",
    "npm": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "sparql-http-client": "^2.4.0",
    "swagger-ui-express": "^4.3.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.12",
    "@types/sparql-http-client": "^2.2.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yargs": "^17.0.9",
    "graphology-types": "^0.24.3",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "sequelize-typescript": "^2.1.3",
    "ts-jest": "^28.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "files": [
    "/dist",
    "/typings"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
